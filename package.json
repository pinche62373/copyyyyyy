    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "winston": "^3.17.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.34.0"
  },
  "engines": {
    "node": ">=22.12.0"
  }
}
