{
  "name": "mdb",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "check": "biome check .",
    "check:fix": "biome check --fix .",
    "check:markdown": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "dev": "react-router dev",
    "knip": "knip",
    "lint-staged": "lint-staged --verbose",
    "migrate": "prisma migrate dev",
    "postmigrate": "npx @snaplet/seed sync",
    "prepare": "husky && tsx .sync/init-upstream.ts",
    "pretest:e2e:run": "npm run build",
    "seed": "npx prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "test": "vitest",
    "typecheck": "react-router typegen && npx prisma generate && tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prisma": {
    "seed": "tsx prisma/seed/seed.ts"
  },
  "@snaplet/seed": {
    "config": "prisma\\seed\\seed.config.ts"
  },
  "lint-staged": {
    "!(app/styles/**/*|package.json)**.*": [
      "biome check --fix --no-errors-on-unmatched"
    ]
  },
  "dependencies": {
    "@epic-web/client-hints": "^1.3.5",
    "@fontsource-variable/inter": "^5.1.1",
    "@hookform/resolvers": "^3.10.0",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@prisma/client": "^6.3.0",
    "@react-router/node": "^7.1.5",
    "@react-router/remix-config-routes-adapter": "^0.0.0-nightly-bf7ecb711-20240911",
    "@react-router/serve": "^7.1.5",
    "@szhsin/react-accordion": "^1.4.0",
    "@szhsin/react-menu": "^4.3.0",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.20.6",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "fs-extra": "^11.3.0",
    "hono-rate-limiter": "^0.4.2",
    "is-localhost-ip": "^2.0.0",
    "isbot": "^5.1.22",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-aria-components": "^1.6.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-modern-drawer": "^1.4.0",
    "react-router": "^7.1.5",
    "react-router-hono-server": "^2.8.2",
    "react-toastify": "^11.0.3",
    "remix-auth": "^4.1.0",
    "remix-auth-form": "^3.0.0",
    "remix-hook-form": "^6.1.3",
    "remix-toast": "^2.0.0",
    "remix-utils": "^8.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.3.1",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.24.1",
    "zod-defaults": "^0.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@faker-js/faker": "^9.4.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-router/dev": "^7.1.5",
    "@sly-cli/sly": "https://pkg.pr.new/jacobparis-insiders/sly/@sly-cli/sly@71",
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "^0.98.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.0",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^19.0.00",
    "@types/react-dom": "^19.00.00",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "2.1.8",
    "autoprefixer": "^10.4.20",
    "binode": "^1.0.5",
    "boxen": "^8.0.1",
    "cross-env": "^7.0.3",
    "happy-dom": "^16.8.1",
    "husky": "^9.1.7",
    "knip": "^5.43.6",
    "lint-staged": "^15.4.3",
    "markdownlint-cli2": "^0.17.2",
    "msw": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.1",
    "prisma": "^6.3.0",
    "react-router-devtools": "^1.1.0",
    "remix-flat-routes": "^0.8.4",
    "rollup-plugin-visualizer": "^5.14.0",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
