{
  "name": "mdb",
  "private": true,
  "sideEffects": false,
  "imports": {
    "#*": "./*"
  },
  "scripts": {
    "build": "remix build",
    "dev": "remix dev -c \"npm run dev:serve\"",
    "dev:serve": "binode --require ./mocks -- @remix-run/serve:remix-serve ./build/index.js",
    "format": "prettier --write .",
    "knip": "knip",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . --max-warnings 0",
    "migrate": "prisma migrate dev",
    "postmigrate": "npx @snaplet/seed sync",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve ./build/index.js",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve ./build/index.js",
    "test": "vitest",
    "pretest:e2e:run": "npm run build",
    "seed": "npx prisma db seed",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prisma": {
    "seed": "tsx prisma/seed/seed.ts"
  },
  "@snaplet/seed": {
    "config": "prisma\\seed\\seed.config.ts"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@fontsource-variable/inter": "^5.1.0",
    "@prisma/client": "^5.21.1",
    "@remix-run/css-bundle": "2.13.1",
    "@remix-run/node": "2.13.1",
    "@remix-run/react": "2.13.1",
    "@remix-run/serve": "2.13.1",
    "@rvf/core": "^6.1.4",
    "@rvf/react": "^6.2.1",
    "@rvf/remix": "^6.2.0",
    "@rvf/set-get": "^6.0.3",
    "@rvf/zod": "^6.0.3",
    "@szhsin/react-menu": "^4.2.2",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.20.5",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "isbot": "^5.1.17",
    "nanoid": "^5.0.7",
    "preline": "^2.5.1",
    "react": "^18.3.1",
    "react-aria-components": "^1.4.1",
    "react-dom": "^18.3.1",
    "remix-auth": "^3.7.0",
    "remix-auth-form": "^1.5.0",
    "remix-themes": "^1.5.1",
    "remix-toast": "^1.2.2",
    "remix-utils": "^7.7.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "tailwind-variants": "^0.2.1",
    "tiny-invariant": "^1.3.3",
    "use-react-router-breadcrumbs": "^4.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "@remix-run/dev": "2.13.1",
    "@snaplet/copycat": "^5.1.0",
    "@snaplet/seed": "^0.98.0",
    "@tailwindcss/forms": "^0.5.9",
    "@testing-library/jest-dom": "^6.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.7.7",
    "@types/prop-types": "^15.7.13",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-testing-library": "^6.4.0",
    "happy-dom": "^15.7.4",
    "knip": "^5.33.3",
    "msw": "^2.4.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "prisma": "^5.21.1",
    "remix-flat-routes": "^0.6.5",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": ">=20.16.0"
  }
}
